{
  "name": "@skeleton-xn/expo",
  "version": "1.0.0",
  "description": "Skeleton for learning how to write modern webapps",
  "keywords": [],
  "main": "./node_modules/expo/AppEntry.js",
  "repository": {
    "type": "git",
    "url": "mabels/skeleton-xn"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/skeleton-xn/issues"
  },
  "homepage": "https://github.com/mabels/skeleton-xn#readme",
  "dependencies": {
    "@skeleton-xn/app-model-context": "1.0.0",
    "@skeleton-xn/models": "1.0.0",
    "@skeleton-xn/agents": "1.0.0",
    "expo": "^33.0.5",
    "native-base": "^2.12.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "react-native-web": "^0.11.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "expo-cli": "^2.19.5",
    "@types/expo": "^32.0.13",
    "@types/react-native": "^0.57.62",
    "babel-preset-expo": "^5.1.1",
    "@babel/core": "^7.4.3",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "24.0.1",
    "@types/node": "^11.9.4",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/uuid": "^3.4.4",
    "babel-loader": "^8.0.5",
    "enzyme-adapter-react-16": "^1.12.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "24.7.1",
    "jest-cli": "24.7.1",
    "jest-emotion": "^10.0.10",
    "prettier": "1.15.3",
    "react-test-renderer": "^16.8.6",
    "react-test-utils": "^0.0.1",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "8.0.1",
    "tslint": "5.15.0",
    "typescript": "^3.5.1",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "compile": "tsc -",
    "depcheck": "depcheck",
    "webpack": "webpack",
    "build": "tsc && webpack",
    "build:watch": "webpack --watch --debug --output-pathinfo --progress",
    "dev": "webpack-dev-server",
    "start": "",
    "test": "jest -c jest.unit.js",
    "lint": "tslint --project .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "expo-start": "expo start",
    "expo-android": "expo start --android",
    "expo-ios": "expo start --ios",
    "expo-eject": "expo eject"
  },
  "publishConfig": {
    "tag": "next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticPrefix": "chore:",
    "devDependencies": {
      "automerge": true
    },
    "packageRules": [
      {
        "packagePatterns": [
          "@types/"
        ],
        "automerge": true
      }
    ]
  },
  "resolutions": {
    "merge": ">=1.2.1",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/prop-types": "15.7.0"
  }
}
